{
  "name": "brand-usa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./out",
    "build-next": "next build",
    "build:prod": "cross-env BUILD_DEST=production npm-run-all --serial clean build-next optimize",
    "build:test": "cross-env BUILD_DEST=preview npm-run-all --serial clean build-next optimize",
    "optimize": "tsx ./optimize-images",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "serve": "npx serve@latest out",
    "start": "npm-run-all --serial build serve"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^11.9.0",
    "gsap": "^3.12.5",
    "lenis": "^1.1.13",
    "next": "^14.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.79.4",
    "tailwind-merge": "^2.3.0",
    "three": "^0.169.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.169.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "glob": "^10.3.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rimraf": "^5.0.7",
    "sharp": "^0.33.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5",
    "webpack-glsl-loader": "^1.0.1"
  }
